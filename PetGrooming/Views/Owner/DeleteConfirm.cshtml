@model PetGrooming.Models.Owner
@{
    ViewBag.Title = "DeleteConfirm";
}

<h2>Are you sure you want to delete @Model.OwnerFname @Model.OwnerLname?</h2>
<div class="navbar">
    <a href="/Owner/Show/@Model.OwnerID">Cancel</a>
</div>
<form method="POST" action="/Owner/Delete/@Model.OwnerID">
    <div class="inpfield"><input type="submit" value="Confirm" /></div>
</form>
