@model PetGrooming.Models.Pet
@{
    ViewBag.Title = "DeleteConfirm";
}

<h2>Are you sure you want to delete @Model.PetName?</h2>
<div class="navbar">
    <a href="/Pet/Show/@Model.PetID">Cancel</a>
</div>
<form method="POST" action="/Pet/Delete/@Model.PetID">
    <div class="inpfield"><input type="submit" value="Confirm" /></div>
</form>
